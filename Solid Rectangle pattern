import java.util.Scanner;

public class LabProgram_01 {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        System.out.println("enter the number : ");
        int row = in.nextInt();
        for (int i =row; i>=1; i--) {
            for(int j = 1; j<=i; j++) {
                System.out.print(" " + j);
            }
            System.out.println( " "); 
        }
    }
}
